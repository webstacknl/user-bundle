{
  "name": "webstack/user-bundle",
  "type": "symfony-bundle",
  "description": "Webstack User Bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Webstack B.V.",
      "email": "info@webstack.nl"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.webstack.nl"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/orm": "^2.10",
    "gedmo/doctrine-extensions": "^3",
    "sensio/framework-extra-bundle": "^5.5|^6.2",
    "symfony/form": "^5.4|^6.0",
    "symfony/framework-bundle": "^5.4|^6.0",
    "symfony/mailer": "^5.4|^6.0",
    "symfony/security-bundle": "^5.4|^6.0",
    "symfony/templating": "^5.4|^6.0",
    "symfony/twig-bundle": "^5.4|^6.0",
    "symfony/validator": "^5.4|^6.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.2",
    "phpstan/phpstan-doctrine": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "rollerworks/password-strength-bundle": "^2.1",
    "symfony/console": "^5.4|^6.0",
    "symfony/event-dispatcher": "^5.4|^6.0",
    "symfony/phpunit-bridge": "^5.4|^6.0",
    "symfony/yaml": "^5.4|^6.0",
    "webstack/admin-bundle": "^3.0"
  },
  "suggest": {
    "rollerworks/password-strength-bundle": "",
    "scheb/2fa-bundle": "",
    "scheb/2fa-email": "",
    "scheb/2fa-google-authenticator": ""
  },
  "autoload": {
    "psr-4": {
      "Webstack\\UserBundle\\": "src/"
    }
  },
  "config" : {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "endroid/installer": true,
      "symfony/flex": true
    }
  }
}
