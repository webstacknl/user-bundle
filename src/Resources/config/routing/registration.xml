<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="webstack_user_registration_register" path="/" methods="GET POST">
        <default key="_controller">webstack_user.registration.controller:register</default>
    </route>

    <route id="webstack_user_registration_confirmed" path="/confirmed" methods="GET">
        <default key="_controller">webstack_user.registration.controller:confirmed</default>
    </route>
</routes>
