<form class="form" action="{{ path("webstack_user_two_factor_authentication_index") }}" method="post">
    <input type="hidden" name="secret" value="{{ secret }}"/>
    <label for="code">
        Authenticatie code
    </label>
    <div>
        <input id="_auth_code" type="number" min=100000 max=999999 autocomplete="off"
               name="code" placeholder="Vul hier uw code in&hellip;"/>
        <p class="submit">
            <input type="submit" value="Activeer"/>
        </p>
    </div>
</form>
